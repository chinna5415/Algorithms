#include <iosteam>
#include <cstdlib>
#define nullptr NULL 

class LinkedList {
	public:
		int data;
		LinkedList *next;
		
		LinkedList(int val, LinkedList *link) {
			this->data = val;
			this->next = link;
		}	
};

class Node {
	public:
		LinkedList *head;
		LinkedList *tail;
		
		Node() {
			this->head = nullptr;
			this->tail = nullptr;
		}
};

int main() {
	int limit;
	cout << "Eenter th"	
}
